{
    "name": "vscode-microtask",
    "version": "1.0.3",
    "publisher": "owenfarrell",
    "engines": {
        "vscode": "^1.29.1"
    },
    "license": "MIT",
    "displayName": "ÂµTask",
    "description": "Runs a task from a subfolder of your workspace",
    "categories": [
        "Other"
    ],
    "keywords": [
        "Task",
        "Ant",
        "Gradle",
        "Maven",
        "MSBuild"
    ],
    "homepage": "https://github.com/owenfarrell/vscode-microtask/",
    "bugs": {
        "url": "https://github.com/owenfarrell/vscode-microtask/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/owenfarrell/vscode-microtask.git"
    },
    "galleryBanner": {},
    "main": "./dist/extension",
    "contributes": {
        "commands": [
            {
                "command": "microtask.runFromHere",
                "title": "Run From Here",
                "category": "Tasks"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "microtask.runFromHere",
                    "group": "microtask"
                }
            ],
            "explorer/context": [
                {
                    "command": "microtask.runFromHere",
                    "group": "microtask"
                }
            ]
        }
    },
    "activationEvents": [
        "onCommand:microtask.runFromHere"
    ],
    "badges": [],
    "markdown": "github",
    "dependencies": {},
    "devDependencies": {
        "ajv": "^6.9.1",
        "ts-loader": "^6.2.2",
        "typescript": "^2.6.2",
        "vscode": "^1.1.36",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "@types/mocha": "^2.2.44",
        "@types/node": "^8.10.60"
    },
    "scripts": {
        "package": "vsce package",
        "vscode:prepublish": "webpack --mode production",
        "test-compile": "tsc -p ./",
        "compile": "webpack --mode development --watch",
        "webpack": "webpack --mode development",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "icon": "icon.png"
}
